{
  "name": "react-translator2",
  "version": "2.0.0",
  "description": "A deadly simple i18n translate plugin for React, ready for Server Side Rendering.",
  "main": "dist/cjs/react-translator.js",
  "module": "dist/es/index.js",
  "unpkg": "dist/umd/react-translator.js",
  "repository": "git@github.com:JounQin/react-translator.git",
  "author": "JounQin <admin@1stg.me>",
  "license": "MIT",
  "scripts": {
    "build:es": "tsc -P tsconfig.build.json --outDir dist/es -d",
    "build:cjs": "cross-env FORMAT=cjs rollup -c",
    "build:umd": "concurrently \"rollup -c\" \"cross-env NODE_ENV=production rollup -c\"",
    "build": "rimraf dist && npm run build:es && concurrently \"npm run build:cjs\" \"npm run build:umd\"",
    "dev": "webpack-dev-server --color --hot --progress",
    "docs": "rimraf docs && cross-env NODE_ENV=production webpack --color --progress -p",
    "lint": "tslint -p . -t stylish '**/*.{ts,tsx}'",
    "test": "rimraf test/__snapshots__ && cross-env NODE_ENV=development jest -u",
    "precommit": "lint-staged && npm run docs && git add -A",
    "prepush": "npm run test",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "types": "dist/es/index.d.ts",
  "babel": {
    "presets": [
      [
        "@babel/env",
        {
          "modules": false
        }
      ],
      "@babel/react"
    ],
    "plugins": [
      "react-hot-loader/babel"
    ],
    "env": {
      "production": {
        "plugins": [
          [
            "transform-react-remove-prop-types",
            {
              "removeImport": true
            }
          ]
        ]
      }
    }
  },
  "jest": {
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "\\/test\\/.*|\\.spec\\.tsx?$",
    "testURL": "http://localhost",
    "moduleFileExtensions": [
      "js",
      "json",
      "node",
      "ts",
      "tsx"
    ]
  },
  "lint-staged": {
    "*.{json,md}": [
      "prettier --write",
      "git add"
    ],
    "*.{ts,tsx}": [
      "tslint -t stylish --fix",
      "git add"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "peerDependencies": {
    "react": "^16.0.0"
  },
  "dependencies": {
    "hoist-non-react-statics": "^3.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.55",
    "@babel/preset-env": "^7.0.0-beta.55",
    "@babel/preset-react": "^7.0.0-beta.55",
    "@types/enzyme": "^3.1.12",
    "@types/enzyme-adapter-react-16": "^1.0.2",
    "@types/fork-ts-checker-webpack-plugin": "^0.4.1",
    "@types/hoist-non-react-statics": "^3.0.0",
    "@types/html-webpack-plugin": "^3.2.0",
    "@types/jest": "^24.0.0",
    "@types/lodash": "^4.14.115",
    "@types/node": "^11.9.0",
    "@types/prop-types": "^15.5.4",
    "@types/react": "^16.4.7",
    "@types/react-dom": "^16.0.6",
    "@types/react-hot-loader": "^4.1.0",
    "@types/react-test-renderer": "^16.0.1",
    "@types/webpack": "^4.4.8",
    "@types/webpack-env": "^1.13.6",
    "babel-loader": "^8.0.0-beta.4",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.14",
    "concurrently": "^3.6.1",
    "cross-env": "^5.2.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "fork-ts-checker-webpack-plugin": "^0.4.3",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "jest": "^23.4.2",
    "lint-staged": "^7.2.0",
    "lodash-es": "^4.17.10",
    "prettier": "^1.14.0",
    "prop-types": "^15.6.2",
    "pug": "^2.0.3",
    "pug-plain-loader": "^1.0.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-hot-loader": "^4.3.4",
    "react-test-renderer": "^16.4.1",
    "rimraf": "^2.6.2",
    "rollup": "^0.65.0",
    "rollup-plugin-buble": "^0.19.2",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^4.0.0",
    "ts-jest": "^23.0.1",
    "ts-loader": "^4.4.2",
    "ts-node": "^7.0.0",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.14.0",
    "tslint-language-service": "^0.9.9",
    "tslint-plugin-prettier": "^1.3.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.0.1",
    "webpack": "^4.16.3",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5"
  }
}
